{
  "name": "@yourorg/libp2p-sdk",
  "version": "0.0.3",
  "description": "A high-end libp2p sdk for p2p project",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "bun --watch ./app.ts",
    "start": "bun ./app.ts",
    "build": "bun run build.mjs",
    "type-docs": "typedoc",
    "show-docs": "npx serve docs/api",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "prepublishOnly": "bun run build && bun type-docs"
  },
  "exports": {
    ".": "./dist/index.js",
    "./Todo": "./dist/modules/Todo.js",
    "./User": "./dist/modules/User.js",
    "./P2PNode": "./dist/p2pnode.js"
  },
  "files": [
    "dist",
    "docs/api",
    "package.json",
    "README.md"
  ],
  "keywords": [
    "vitest",
    "typedoc",
    "react",
    "bun"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/bun": "latest",
    "@vitest/coverage-v8": "4.0.8",
    "@vitest/ui": "4.0.8",
    "bun-plugin-dts": "^0.3.0",
    "typedoc": "^0.28.14",
    "vitest": "^4.0.8"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "typedocOptions": {
    "entryPointStrategy": "expand",
    "entryPoints": [
      "src"
    ],
    "out": "docs/api",
    "theme": "default",
    "includeVersion": true,
    "hideGenerator": true
  },
  "dependencies": {
    "@chainsafe/libp2p-gossipsub": "^14.1.1",
    "@chainsafe/libp2p-noise": "^16.1.4",
    "@chainsafe/libp2p-yamux": "^7.0.4",
    "@headlessui/react": "^2.0.3",
    "@libp2p/autonat": "^2.0.37",
    "@libp2p/bootstrap": "^11.0.43",
    "@libp2p/circuit-relay-v2": "^3.2.20",
    "@libp2p/identify": "^3.0.37",
    "@libp2p/interface-pubsub": "^4.0.1",
    "@libp2p/logger": "^5.1.21",
    "@libp2p/peer-id-factory": "^4.2.0",
    "@libp2p/ping": "^3.0.8",
    "@libp2p/pubsub-peer-discovery": "^11.0.2",
    "@libp2p/tcp": "^10.1.17",
    "@libp2p/webrtc": "^5.2.20",
    "@libp2p/websockets": "^9.2.17",
    "@libp2p/webtransport": "^5.0.47",
    "@multiformats/multiaddr": "^12.3.0",
    "@multiformats/multiaddr-matcher": "^1.2.4",
    "@yourorg/libp2p-sdk": "^0.0.1",
    "libp2p": "^2.8.12"
  }
}
